{
  "scripts": {
    "dev": "set NODE_ENV=development&& eslint ./src --ext .ts && nodemon -r tsconfig-paths/register src/main.ts -L",
    "prod": "set NODE_ENV=production&& node build/main.js",
    "test-app": "set NODE_ENV=test&& eslint ./src --ext .ts && nodemon -r tsconfig-paths/register src/main.ts -L",
    "test": "jest",
    "unit-tests": "jest ./src/tests/unit_tests/CreateUserHandler.unit.test.ts --coverage",
    "integration-tests": "jest ./src/tests/integration_tests --coverage",
    "system-tests": "jest ./src/tests/system_tests --coverage",
    "api-tests": "jest ./src/tests/api_tests --coverage",
    "lint": "eslint ./src --ext .ts",
    "build": "tsc -p tsconfig.build.json && tsc-alias && cp ./src/resources/log4js-config.json ./build/resources"
  },
  "dependencies": {
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "db-migrate": "^0.11.14",
    "db-migrate-mysql": "^2.3.2",
    "dotenv": "^16.4.0",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "log4js": "^6.9.1",
    "mysql2": "^3.9.1",
    "reflect-metadata": "^0.2.1",
    "tsyringe": "^4.8.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.11.5",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "eslint": "^8.56.0",
    "eslint-config-standard-with-typescript": "^43.0.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-promise": "^6.1.1",
    "jest": "^29.7.0",
    "nodemon": "^3.0.3",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.8",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  }
